        "docs":
          { "summary": "Reset member password",
            "description":
              [ "Replaces the member password with a random password and it requires the member to reset his password to login" ],
            "links": [],
            "examples":
              [ { "title": "basic usage",
                  "body":
                    [ "import wixMembersBackend from \"wix-members-backend\";",
                      "",
                      "export function resetPassword() {",
                      "  const resetPasswordRequest = {",
                      "    contactId: \"a string\"",
                      "  };",
                      "",
                      "  return wixMembersBackend",
                      "    .resetPassword(resetPasswordRequest)",
                      "    .then((response) => {",
                      "      console.log(response);",
                      "    })",
                      "    .catch((error) => {",
                      "      console.error(error);",
                      "    });",
                      "}" ],
                  "extra":
                    {  } } ],
